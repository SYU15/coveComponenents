{
  "name": "reactTVComponents",
  "version": "0.0.1",
  "dependencies": {
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "ics": "^0.4.1",
    "jquery": "^2.1.4",
    "moment": "^2.10.6",
    "moment-range": "^2.0.3",
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "babelify": "^6.3.0",
    "browser-sync": "^2.9.4",
    "browserify": "^10.2.4",
    "browserify-shim": "^3.8.10",
    "gulp": "^3.9.0",
    "gulp-uglify": "^1.4.1",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.5.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.4.0",
    "yargs": "^3.25.0"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "tab": "./bower_components/semantic/dist/components/tab.min.js",
    "dropdown": "./bower_components/semantic/dist/components/dropdown.min.js"
  },
  "browserify-shim": {
    "tab": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "dropdown": {
      "depends": [
        "jquery:jQuery",
        "./bower_components/semantic-ui/dist/components/transition.min.js:transition"
      ]      
    }
  }
}
